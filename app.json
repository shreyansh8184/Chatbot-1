{
  "name": "Chat Bot",
  "description": "",
  "env": {
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true
    },
    "TOKEN": {
      "description": "Your BOT TOKEN from @Botfather",
      "required": true
    },
    "CF_API_KEY": {
      "description": "Your access key from https://coffeehouse.intellivoid.net/",
      "required": true
    },
    "ENV": {
      "description": "Set this to true if using heroku",
      "value": "True",
      "required": true
    },
    "NAME": {
      "description": "Bot will respond to this name.",
      "required": true
   }},
   "addons": [{
    "plan": "Heroku Postgres",
    "options": {
      "version": "12"
    }
  }],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
   ]
}
